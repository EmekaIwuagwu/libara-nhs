{
  "name": "libaranhs",
  "version": "1.0.0",
  "description": "Automate NHS job applications with LibaraNHS - A full-stack application for NHS Scotland and NHS England job automation",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "build:css": "npx tailwindcss -i ./src/public/css/input.css -o ./src/public/css/output.css --minify",
    "watch:css": "npx tailwindcss -i ./src/public/css/input.css -o ./src/public/css/output.css --watch",
    "db:migrate": "node scripts/migrate.js",
    "download:images": "node scripts/download-images.js"
  },
  "keywords": [
    "nhs",
    "jobs",
    "automation",
    "puppeteer",
    "healthcare"
  ],
  "author": "LibaraNHS",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-mysql-session": "^3.0.0",
    "ejs": "^3.1.9",
    "mysql2": "^3.6.5",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "puppeteer": "^21.6.1",
    "@google/generative-ai": "^0.1.3",
    "nodemailer": "^6.9.7",
    "express-validator": "^7.0.1",
    "connect-flash": "^0.1.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
